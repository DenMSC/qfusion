textures/blx_grey/blx_grey_wall_1
{
	qer_editorimage textures/blx_grey/blx_grey_wall_1
	{
		map $lightmap
		rgbGen identity
	}
	{
		map textures/blx_grey/blx_grey_wall_1
		blendfunc filter
	}
	{
		map textures/blx_grey/blx_grey_wall_1_alpha
		blendFunc add
	}
}

