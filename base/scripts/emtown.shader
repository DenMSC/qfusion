{
	qer_trans 0.5
	qer_editorimage textures/grates/fence01
	surfaceparm trans
	surfaceparn nolightmap
	surfaceparm nomarks
	//surfaceparm	nonsolid
	//surfaceparm alphashadow
	sort additive
	cull none
	nomipmaps
	q3map_forceMeta
	q3map_lightmapsamplesize 64

	{
		map textures/grates/fence01
		alphatest 0.5
	}
}

