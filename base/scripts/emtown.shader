textures/emtown/vine1
{
	qer_editorimage textures/emtown/vine1
	qer_trans 0.9
	surfaceparm nolightmap
	surfaceparm nomarks
	surfaceparm nodlight
	surfaceparm nonsolid
	surfaceparm trans
	polygonOffset

	{
		detail
		map textures/emtown/vine1
		blendFunc blend
		rgbGen Vertex
	}
}

